          <omgdc:Bounds height="14.0" width="24.0" x="90.0" y="309.0"></omgdc:Bounds>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="109.0" y="389.0"></omgdi:waypoint>
        <omgdi:waypoint x="188.0" y="390.0"></omgdi:waypoint>
        <bpmndi:BPMNLabel>
          <omgdc:Bounds height="14.0" width="42.0" x="109.0" y="389.0"></omgdc:Bounds>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="89.0" y="409.0"></omgdi:waypoint>
        <omgdi:waypoint x="89.0" y="489.0"></omgdi:waypoint>
        <bpmndi:BPMNLabel>
          <omgdc:Bounds height="14.0" width="42.0" x="89.0" y="409.0"></omgdc:Bounds>
        </bpmndi:BPMNLabel>
